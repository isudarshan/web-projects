<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.hms.domain.sub.Pharmacist" table="pharmacy">
        <id column="id" name="id" type="java.lang.Long">
            <generator class="native" />
        </id>


        <property column="photograph" name="photograph" type="binary" />

        <property column="pharmacistName" name="pharmacistName" type="java.lang.String" />



    </class>
</hibernate-mapping>
