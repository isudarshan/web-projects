<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
  
<hibernate-mapping>
    <class name="com.hms.domain.sub.RegisteredPatient" table="patient">
        <id column="id" name="id" type="java.lang.Long">
            <generator class="native" />
        </id>
		
        <property column="guid" name="guid" type="java.lang.String" />
        <property column="createdOn" name="createdOn" type="java.util.Date" />
        <property column="createdBy" name="createdBy" type="java.lang.String" />
        <property column="modifiedBy" name="modifiedBy" type="java.lang.String" />
        <property column="modifiedOn" name="modifiedOn" type="java.util.Date" />
        <property column="active" name="active" type="java.lang.Long" />
        <property column="photograph" name="photograph" type="binary" />

        <property column="firstName" name="firstName" type="java.lang.String" />
        <property column="lastName" name="lastName" type="java.lang.String" />
        <property column="gender" name="gender" type="java.lang.String" />
        <property column="height" name="height" type="java.lang.String" />
        <property column="weight" name="weight" type="java.lang.String" />
        <property column="address" name="address" type="java.lang.String" />
        <property column="mobileNumber" name="mobileNumber" type="java.lang.String" />
        <property column="observation" name="observation" type="java.lang.String" />
        <property column="requestedConsultant" name="requestedConsultant" type="java.lang.String" />
        <property column="dateOfBirth" name="dateOfBirth" type="java.lang.String" />
        <property column="age" name="age" type="java.lang.String" />
        <property column="medicalHistory" name="medicalHistory" type="java.lang.String" />
        <property column="consultationFee" name="consultationFee" type="java.lang.Double" />
            
		
    </class>
</hibernate-mapping>
